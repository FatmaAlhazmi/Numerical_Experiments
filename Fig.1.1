   "source": [
    "# Plot invariants\n",
    "def invariant(y):\n",
    "    u, v = y\n",
    "    return np.log(u) - u + 2*np.log(v) - v\n",
    "\n",
    "x = np.linspace(0, 4, 100)\n",
    "y = np.linspace(0, 4, 100)\n",
    "X, Y = np.meshgrid(x, y)\n",
    "Z = invariant([X, Y])\n",
    "plt.figure()\n",
    "plt.contour(X, Y, Z, levels=np.arange(-6, 7), colors='k', linestyles='solid')\n",
    "plt.xlabel('Prey')\n",
    "plt.ylabel('Predator')\n",
    "plt.title('Lotka-Volterra Invariant Curves')\n",
    "\n",
    "plt.show()\n"
   ]
